{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "ESNext" /* Specify ECMAScript target version: 'ES3' (default), 'ES5', 'ES2015', 'ES2016', 'ES2017', 'ES2018', 'ES2019' or 'ESNEXT'. */,
      "moduleResolution": "node",
      "jsx": "react",
      "allowJs": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
    "include": ["./src/**/*"]
  }
  